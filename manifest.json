{
    "name": "Advanced REST client - Dev 3",
    "description": "The web developers helper program to create and test custom HTTP requests.",
    "icons": {
        "16": "img/arc_icon_16.png",
        "128": "img/arc_icon_128.png"
    },
    "version": "4.0",
    "manifest_version": 2,
    "permissions": [
        "<all_urls>",
        "storage",
        "identity",
        "unlimitedStorage",
        "clipboardWrite",
        {"socket": ["tcp-connect:*:*"]}
    ],
    "oauth2": {
        "client_id": "10525470235.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/drive.file",
            "https://www.googleapis.com/auth/drive.readonly.metadata",
            "https://www.googleapis.com/auth/userinfo.profile"
        ]
    },
    "app": {
        "background": {
            "scripts": ["js/event.js", "js/idbstore.min.js"]
        }
    }
}